<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#00FFFFFF" tools:context=".Akun_saya">


    <RelativeLayout
        android:id="@+id/head_akunsaya"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="15dp">

        <TextView
            android:fontFamily="@font/dm_sans_bold"
            android:id="@+id/textView29"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="30dp"
            android:layout_toEndOf="@+id/imageButton7"
            android:text="Akun Saya"
            android:textSize="20dp"/>
        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:backgroundTint="#00FFFFFF"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/back"/>


    </RelativeLayout>
    <androidx.cardview.widget.CardView
        android:id="@+id/card1"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_below="@+id/head_akunsaya"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="10dp">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:backgroundTint="#00FFFFFF"
                app:srcCompat="@drawable/personprofil"/>
            <TextView
                android:fontFamily="@font/inter_medium"
                android:id="@+id/textView9"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_toEndOf="@+id/imageView4"
                android:text="Nomor Telepon"/>
            <TextView
                android:fontFamily="@font/inter_medium"
                android:id="@+id/textView30"
                android:layout_toStartOf="@+id/imageButton10"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="0852-72xx-xxxx"/>
            <ImageButton
                android:id="@+id/imageButton10"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#00FFFFFF"
                app:srcCompat="@drawable/backprofil"/>

        </RelativeLayout>



    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:id="@+id/card2"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_below="@+id/card1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:padding="10dp">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#00FFFFFF"
                app:srcCompat="@drawable/personprofil"/>
            <TextView
                android:fontFamily="@font/inter_medium"
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_toEndOf="@+id/imageView5"
                android:text="Email"/>
            <TextView
                android:fontFamily="@font/inter_medium"
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView12"
                android:layout_marginStart="20dp"
                android:layout_toEndOf="@+id/imageView5"
                android:text="novan0189@gmail.com"/>
            <ImageButton
                android:layout_toStartOf="@+id/imageButton14"
                android:layout_centerVertical="true"
                android:id="@+id/imageButton9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#00FFFFFF"
                app:srcCompat="@drawable/peringatan"/>
            <ImageButton
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:id="@+id/imageButton14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#00FFFFFF"
                app:srcCompat="@drawable/backprofil"/>
        </RelativeLayout>


    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:id="@+id/card3"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_below="@+id/card2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:padding="10dp">
            <ImageView
                android:layout_centerVertical="true"
                android:id="@+id/imageView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#00FFFFFF"
                app:srcCompat="@drawable/personprofil"/>
            <TextView
                android:fontFamily="@font/inter_medium"
                android:layout_centerVertical="true"
                android:id="@+id/textView28"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_toEndOf="@+id/imageView6"
                android:text="Tanggal lahir"/>
            <TextView
                android:fontFamily="@font/inter_medium"
                android:id="@+id/textView31"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginEnd="0dp"
                android:layout_toStartOf="@+id/imageButton15"
                android:hint="29 September 2003"/>
            <ImageButton
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:id="@+id/imageButton15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#00FFFFFF"
                app:srcCompat="@drawable/backprofil"/>
        </RelativeLayout>



    </androidx.cardview.widget.CardView>


</RelativeLayout>